{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "PS-WASM",
	"version": "0.2",
	"icons": { "128": "logo.png" },
	"description": "Rendering PostScript using GhostScript in WebAssembly.",
	"manifest_version": 2,
	"background": {
		"scripts": [
		"background.js"
		]
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"tabs",
		"<all_urls>",
		"webRequest",
		"webRequestBlocking"
	]
}
